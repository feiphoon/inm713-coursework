                  rdf:type owl:FunctionalProperty ;
                  dc:creator "fei"^^xsd:string .


###  http://www.city.ac.uk/ds/inm713/feiphoon#stateHasPlace
fp:stateHasPlace rdf:type owl:ObjectProperty ;
                 dc:creator "fei"^^xsd:string .


