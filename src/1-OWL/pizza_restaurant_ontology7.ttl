###  http://www.city.ac.uk/ds/inm713/feiphoon#hasIngredientAsTopping
fp:hasIngredientAsTopping rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf fp:hasIngredient ;
                          rdfs:domain fp:Pizza ;
                          rdfs:range fp:Ingredient ;
